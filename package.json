{
  "name": "bia-challenge",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "launch": "ts-node --project scripts/tsconfig.json -r tsconfig-paths/register scripts/launch",
    "launch:api": "yarn launch --withCountryTarget=api",
    "sync:envs": "ts-node --project scripts/tsconfig.json -r tsconfig-paths/register scripts/sync-envs",
    "build": "next build",
    "start": "next start",
    "lint": "eslint ./src ./__tests__",
    "lint:fix": "eslint --fix ./src ./__tests__",
    "prettier": "prettier \"**/*.+(js|json|ts|tsx)\"",
    "check-types": "tsc --project tsconfig.json --pretty --noEmit",
    "test": "vitest run",
    "validate": "yarn lint && yarn check-types && yarn test",
    "prepare": "husky"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/material": "^7.2.0",
    "@mui/material-nextjs": "^7.2.0",
    "@tanstack/react-query": "^5.84.1",
    "@mui/icons-material": "5.14.1",
    "axios": "^1.11.0",
    "next": "15.4.5",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "commander": "^14.0.0",
    "concurrently": "^9.2.0",
    "eslint": "^9",
    "eslint-config-next": "15.4.5",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "author": "Brayan Arango <br3y1n@hotmail.es>",
  "license": "MIT"
}
